{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint . --max-warnings 50",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint .",
    "serve": "firebase emulators:start --only functions --port 5001",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "engines": {
    "node": "20"
  },
  "main": "index.js",
  "dependencies": {
    "axios": "^1.10.0",
    "dotenv": "^16.6.0",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.3.2",
    "googleapis": "^137.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.1",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.3.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "private": true,
